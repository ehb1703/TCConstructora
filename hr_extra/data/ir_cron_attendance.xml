<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Cron Job: Procesar Registros de Asistencia Pendientes -->
        <record id="ir_cron_process_attendance_logs" model="ir.cron">
            <field name="name">Procesar Registros de Asistencia Pendientes</field>
            <field name="model_id" ref="model_ctrol_asistencias"/>
            <field name="state">code</field>
            <field name="code">model.process_pending_logs()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="active" eval="False"/>
            <field name="user_id" ref="base.user_admin"/>
        </record>
        
    </data>
</odoo>
